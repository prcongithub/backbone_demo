body {
	background: #CCCCBF;
}

.wrapper{
	width:100%;
	margin:auto;
	padding:0px;
}

/*********header css**********/

.header-wrap{
	background-color:#AAC46C;
	width:100%;
}

.header {
	box-shadow: 0 0 4px 0 #333;
}

.header_user_name {
	float: left;
	color: black;
	width:280px;
	font-weight: bold;
  position:relative;
  cursor:pointer;
  z-index: 2;
}

.drop-down{
	background:transparent url('<%= asset_path('user_bg.png') %>') repeat;
	padding:0px 10px 10px 10px;
	width: 75%;
	position:absolute;
	color:#fff;
	border-bottom-left-radius:5px;
	border-bottom-right-radius:5px;
}

.drop-down.click{
	background:transparent url('<%= asset_path('user_bg.png') %>') repeat;
	padding:0px 10px 10px 10px;
	width: 75%;
	position:absolute;
	color:#fff;
	min-height: 155px !important;
  -webkit-border-bottom-left-radius: 5px;
  -moz-border-radius-bottomleft: 5px;
  border-bottom-left-radius: 5px;
  -webkit-border-bottom-right-radius: 5px;
  -moz-border-radius-bottomright: 5px;
  border-bottom-right-radius: 5px;
}

#user_drop_down{
	padding:15px 8px;
	font-weight:normal;
	font-size:15px;
	background:#566825 url('<%= asset_path('down-arrow.png') %>')no-repeat;
	background-position: 180px 15px;
}

#user_drop_down.click{
	
}

.setting_drop_down{
	position:absolute;
	background:#fff;
	padding:10px;
	width: 82%;
	font-weight:normal;
	font-size:14px;
	line-height:15px;	
}

.setting_drop_down ul li{
	padding:5px;
}

.setting_drop_down ul li:hover{
	background:#798455;
	color:#fff;
}

.setting_drop_down ul li a{
	color:#566825;
	padding:5px 10px;
}

.setting_drop_down ul li a:hover{
	color:#fff;
	text-decoration:none;
}

.header_user_forms {
	float: right;
	margin-right:10px;
	font-weight: bold;
  position:relative;
  cursor:pointer;
  z-index: 2;
}

.header_user_forms a {
	color: black;
}

.header_user_options {
	width: 140px;
	padding: 10px 5px;
	display: block;
	position: absolute;
	right: 0;
	top: 40px;
	z-index: 1;
	text-align: center;
	-webkit-border-bottom-left-radius: 1em;
  -moz-border-radius-bottomleft: 1em;
  border-bottom-left-radius: 1em;
  -webkit-border-bottom-right-radius: 1em;
  -moz-border-radius-bottomright: 1em;
  border-bottom-right-radius: 1em;
	background:#AAC46C;
}

.header_user_options a{
	font-weight:bold;
	color:#333;
}

.logo-div{
	float:left;
	width:50%;
}

.right-header-div{
	width: 540px;
	float:right;
	position:relative;
}

.search-div{
	float:left;
	margin-top:5px;
	margin-right:5px;
}

.sign-in-div{
	margin:24px 80px 10px 10px;
	float:right;
}

.user-sign-in{
	float:left;
	margin-left:10px;
	padding:6px 6px;
	border-radius:5px;
	font-weight:bold;
	font-size:16px;
	border:1px solid #76acfc;
	background: rgb(225,255,255); /* Old browsers */
	/* IE9 SVG, needs conditional override of 'filter' to 'none' */
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top,  rgba(225,255,255,1) 0%, rgba(253,255,255,1) 0%, rgba(225,255,255,1) 10%, rgba(225,255,255,1) 16%, rgba(230,248,253,1) 30%, rgba(200,238,251,1) 54%, rgba(190,228,248,1) 75%, rgba(177,216,245,1) 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(225,255,255,1)), color-stop(0%,rgba(253,255,255,1)), color-stop(10%,rgba(225,255,255,1)), color-stop(16%,rgba(225,255,255,1)), color-stop(30%,rgba(230,248,253,1)), color-stop(54%,rgba(200,238,251,1)), color-stop(75%,rgba(190,228,248,1)), color-stop(100%,rgba(177,216,245,1))); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  rgba(225,255,255,1) 0%,rgba(253,255,255,1) 0%,rgba(225,255,255,1) 10%,rgba(225,255,255,1) 16%,rgba(230,248,253,1) 30%,rgba(200,238,251,1) 54%,rgba(190,228,248,1) 75%,rgba(177,216,245,1) 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  rgba(225,255,255,1) 0%,rgba(253,255,255,1) 0%,rgba(225,255,255,1) 10%,rgba(225,255,255,1) 16%,rgba(230,248,253,1) 30%,rgba(200,238,251,1) 54%,rgba(190,228,248,1) 75%,rgba(177,216,245,1) 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  rgba(225,255,255,1) 0%,rgba(253,255,255,1) 0%,rgba(225,255,255,1) 10%,rgba(225,255,255,1) 16%,rgba(230,248,253,1) 30%,rgba(200,238,251,1) 54%,rgba(190,228,248,1) 75%,rgba(177,216,245,1) 100%); /* IE10+ */
	background: linear-gradient(to bottom,  rgba(225,255,255,1) 0%,rgba(253,255,255,1) 0%,rgba(225,255,255,1) 10%,rgba(225,255,255,1) 16%,rgba(230,248,253,1) 30%,rgba(200,238,251,1) 54%,rgba(190,228,248,1) 75%,rgba(177,216,245,1) 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#e1ffff', endColorstr='#b1d8f5',GradientType=0 ); /* IE6-8 */

}

.sign-in-div a{
	color:#000;
}

.form-id-search-div{
	float:left;
	width:200px;
	margin:17px 10px 5px 10px;
}

.form-id-text{
	border-radius: 10px;
	width:140px;
	margin: 7px 0 0 10px;
}

/*******end********/

/*****content css*********/

.content-wrap{
	padding:0px 0px;
	min-height:485px;
}

.home-content{
	width: 100%;
	background: #fff;
}

.home-content-top{
	height:220px;
	padding: 10px;
	font-size: 25px;
	line-height:25px;
	font-weight: bold;
}

.home-content-bottom{
	padding:5px 0px;
	background:#fff;
}

.home-key-points-div{
	float:left;
	padding: 0px 0px 0px 10px;
	margin: 5px 5px 5px 0px;
	width: 32%;
}

.yellow-box{
	background:#FEFF73;	
}

.box-content-div{
	padding:10px;
	box-shadow: 0 4px 2px -2px gray;
	font-size:24px;
	line-height:32px;
	margin: 10px 5px;
}

.lg-black{
	font-weight:bold;
	font-size:29px;
	line-height:29px;
}

.orange-box{
	background:#F7A22E;
}

.green-box{
	background:#CAED9D;
}

.home-try-it-div{
	float:left;
	width: 14%;
	padding: 5px 0px;
}

.tri-it-img-div{
	margin:110px 5px;
	z-index:100;
	position:absolute;
}

.home-demo-form-div{
	margin: 0;
	float: right;
	width: 50%;
	padding: 0px;
}

/********end********/

/**********template page css************/

.form-template-div{
	padding:30px 20px 30px 35px;
}

.create-new-btn-div{
	padding: 10px 0px;
	margin: 10px 10px;
	font-weight: bold;
	font-size: 20px;
}

.new-form-option-div{
	padding:10px 0px;
	margin:10px 10px;
	min-height:130px;
}

a .form-option-div{
	color:#6B6B6B;
	float:left;
	width:100px;
	height:100px;
	margin:0px 10px;
	padding:10px 10px 10px 10px;
	box-shadow:6px 7px 27px -3px #333;
	text-align:center;
	background: white;
}

a:hover .form-option-div{
	padding: 202px 20px 20px 20px;
	margin: -9px 0px;
}


.form-option-div p{
	font-weight:bold;
	font-size:14px;
	line-height:14px;
}
/******end********/

/*******user template css********/

.form-option-div-1 li{
	position: relative;
	list-style:none;
	float:left;
	margin:0px 10px;
	padding:10px 10px 10px 10px;
	box-shadow:6px 7px 27px -3px #333;
	text-align:center;
	background: white;
}

.form-option-div-1 li:hover{
	padding: 20px 20px 20px 20px;
	margin: -9px 0px;
}

.form-option-div-1 li a{
	font-weight:bold;
	font-size:14px;
	line-height:14px;
	color:#6B6B6B;
}

.form-option-div-1 li a div{
	min-width: 100px;
	max-width: 100px;
	min-height: 100px;
	max-height: 100px;
}


.form-option-div-1 li a:hover{
	text-decoration:none;
}

.yellwo-strip-div{
	background:#FFFB00;
	padding:6px 10px;
	margin: 0px 18px;
	width: 700px;
	font-size:17px;
	font-weight:bold;
}

.form-edit-option-div{
	margin:15px 30px;
}

.form-edit-option-div ul li{
	position: relative;
	padding:3px 8px;
	margin:0px 4px;
	float:left;
	border:1px solid #ccc;
	border-radius:25px;
	background-color:#AAC46C;
}

.form-edit-option-div ul li:hover{
	box-shadow:0px 1px 4px -1px #333;
}

.form-edit-option-div ul li a{
	color:#fff;
	font-weight:bold;
}

.share-copy-edit-delete {
	position: absolute;
	bottom: 0px;
	width: 100%;
	text-align: center;
	background: black;
	left: 0;
	height: 25px;
}
/******end********/


