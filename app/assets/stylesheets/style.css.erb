.footer-social-div{
	float:right;
	width:20%;
	padding: 5px 0px 23px 20px;
	margin-left: 10px;
	border-left: 1px solid #CCC;
	box-shadow: -12px 1px 18px -8px #CCC;
}

.footer-demo-form-div span{
	font-weight:bold;
	font-size:15px;
}

.footer-link-div{
	padding:5px 0px;
	float:left;
	width:40%;
	border-right: 1px solid #CCC;
	box-shadow: 9px 0px 10px -7px #CCC;	
}

.footer-links ul li{
	float:left;
	padding:0px 5px;
	margin:5px 5px 5px 0px;
}

.footer-links ul li a{
	color:#333;
	font-weight:bold;
}

.footer-links ul li a:hover{
	text-decoration:none;
}

.copy-right-div{
	font-size:10px;
	padding:5px 0px 0px 5px;
}

.footer-statistics-div{
	float:left;
	text-align: center;
	width:35%;
	margin:0px 10px;
	padding:5px 0px;
}
/*******end********/

/********dashboard css**********/
.container {
	position:relative;
	width: 900px;	
	margin: auto;
}

header {
	margin-top: 5px;
	padding: 10px;
	background: #fff;
}

header nav {
	float: right;
}

header nav ul {
	list-style-type:none;
}

header nav ul li {
	display:inline-block;
	padding: 5px;
}

section {
	margin-top: 10px;
}

footer {
	margin-top: 20px;
	padding: 10px;
	background: #fff;
	text-align: center;
}

footer nav {
}

footer nav ul {
	list-style-type:none;
}

footer nav ul li {
	display:inline-block;
	padding: 5px;
}

.round {
	border: solid #ddd 1px;
	-moz-border-radius: 20px;
	-webkit-border-radius: 20px;
	border-radius: 20px;
}

.content {
	min-height: 440px;
	width: 74%;
	background: #fff;
	margin-left: 20px;
	padding: 10px;
	float: left;
}

.sidebar {
	width: 18%;
	background: #fff;
	padding: 10px;
	float:left;
}

.clr{
	clear:both;
	height:0px;
	width:0px;
}

.notic_div{
	position: absolute; 
	text-align: center; 
	width: 100%; 
	top: 8px;
	z-index:1000;
}

.notic_div span{
	background: maroon;
	padding: 5px;
	border-radius: 5px;
	color: white;
	font-size: 20px;
}


